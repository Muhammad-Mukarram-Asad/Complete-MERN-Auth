{
    "version": 2,
    "builds": [
      {
        "src": "Backend/server.js",   
        "use": "@vercel/node"
      },
      {
        "src": "frontend/package.json",   
        "use": "@vercel/static-build",
        "config": {"distDir": "frontend/dist"}  
      }
    ],
   "routes": [
  {
    "src": "/api/(.*)",
    "dest": "/Backend/server.js"
  },
         {
    "src": "/api/users/(.*)",
    "dest": "/Backend/server.js"
  },
  {
    "src": "/(.*)",
    "dest": "/frontend/$1"
  }
]

  }
  
